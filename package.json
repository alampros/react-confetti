{
  "name": "react-confetti",
  "version": "1.1.0",
  "description": "React component to draw confetti for your party.",
  "main": "lib/react-confetti.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:alampros/react-confetti.git"
  },
  "keywords": [
    "react",
    "confetti",
    "component",
    "react-component"
  ],
  "author": "Aaron Lampros <alampros@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alampros/react-confetti/issues"
  },
  "homepage": "https://github.com/alampros/react-confetti#readme",
  "files": [
    "README.md",
    "LICENSE.md",
    "lib/"
  ],
  "scripts": {
    "build": "webpack",
    "clean": "rm -rf lib/* docs/bundle.js",
    "lint": "eslint src/ --ext .js --ext .jsx || true",
    "prepublish": "npm run clean && npm run lint && npm run build"
  },
  "peerDependencies": {
    "react": "0.14.x || ^15.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.11",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-register": "^6.18.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.8.0",
    "eslint-plugin-sorting": "^0.3.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "webpack": "^2.2.0-rc.4",
    "webpack-dev-server": "^2.2.0-rc.0"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "stage-1",
      "react"
    ],
    "plugins": [
      "transform-es2015-modules-commonjs"
    ],
    "env": {
      "production": {
        "plugins": [
          "transform-react-remove-prop-types"
        ]
      }
    }
  }
}
